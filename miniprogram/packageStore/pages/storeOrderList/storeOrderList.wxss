.title {
  display: flex;
  align-items: center;
  height: 90rpx;
  width: 750rpx;
  z-index: 200;
}

.condition {
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 32rpx;
}

.orderSection {
  height: 360rpx;
  width: 720rpx;
  margin: 10rpx 15rpx;
  border-radius: 16rpx;
}

.order_price {
  align-items: center;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1rpx solid #DDDDDD;
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.order_img {
  height: 140rpx;
  width: 140rpx;
  margin: 10rpx;
}

.order_bottom {
  margin: 10rpx;
  height: 90rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
}

.order_btn {
  width: 160rpx;
  height: 50rpx;
  text-align: center;
  align-items: center;
  line-height: 50rpx;
  border-radius: 25rpx;
  border: 1rpx solid #079547;
  margin-left: 10rpx;
}

.input_txt {
  color: #606060;
  font-size: 28rpx;
  text-align: left;
  width: 450rpx;
  margin-left: 20rpx;
}

.picker {
  color: #606060;
  font-size: 28rpx;
  width: 450rpx;
  height: 80rpx;
  text-align: left;
  line-height: 80rpx;
  margin-left: 20rpx;
}

.confirmLogdata {
  height: 50rpx;
  border-radius: 25rpx;
  width: 200rpx;
  background-color: #079547;
  color: #ffffff;
  font-size: 32rpx;
  margin: 30rpx 225rpx;
  line-height: 50rpx;
  text-align: center;
}